{
  "editor.inlineSuggest.enabled": true,
  "files.autoSave": "afterDelay",
  "activitusbar.alignment": "Left",
  "activitusbar.views": [
    {
      "name": "explorer",
      "codicon": "explorer-view-icon"
    },
    {
      "name": "search",
      "codicon": "search-view-icon"
    },
    {
      "name": "scm",
      "codicon": "source-control-view-icon"
    },
    {
      "name": "debug",
      "codicon": "run-view-icon"
    },
    {
      "name": "extensions",
      "codicon": "extensions-view-icon"
    },
    {
      "name": "copilotSidebar.explain.focus",
      "codicon": "lightbulb"
    },
    {
      "name": "targetsWsl.focus",
      "codicon": "remote-explorer"
    }
  ],
  "editor.cursorBlinking": "phase",
  "editor.cursorSmoothCaretAnimation": "on",
  "editor.cursorWidth": 3,
  "editor.scrollbar.horizontal": "visible",
  "editor.smoothScrolling": true,
  "editor.linkedEditing": true,
  "markdown-preview-enhanced.previewTheme": "one-dark.css",
  "editor.fontSize": 15,
  "editor.wordWrap": "on",
  "editor.inlineSuggest.showToolbar": "always",
  "workbench.colorTheme": "One Dark Pro Flat",
  "apc.imports": [
    {
      "rel": "stylesheet",
      "href": "https://cdn.jsdelivr.net/gh/orioncactus/pretendard/dist/web/static/pretendard.css"
    }
  ],
  "apc.stylesheet": {
    "body": {
      "background-image": "url(https://gitcdn.fmt.kr/vscode_background.jpg), linear-gradient(to top,rgba(0, 0, 0, 0.6), rgba(0, 0, 0, 0.9))",
      "background-size": "cover",
      "background-blend-mode": "multiplay",
      "background-repeat:": "no-repeat",
      "backdrop-filter": "brightness(0.15) saturate(150%)"
    },
    ".monaco-workbench .part.editor>.content .editor-group-container>.title div.tabs-container": "padding-right: 80px;",
    ".sticky-widget": "background: rgba(0, 0, 0, .6) !important;",
    ".part.banner": "-webkit-user-select: none; -webkit-app-region: drag; padding-right: 140px;",
    ".monaco-link": "-webkit-app-region: no-drag;",
    ".action-item": "-webkit-app-region: no-drag;"
  },
  "apc.electron": {
    "titleBarStyle": "hidden",
    "titleBarOverlay": {
      "color": "#00080b",
      "symbolColor": "#ffffff",
      "height": 28
    }
  },
  "apc.activityBar": {
    "size": 28,
    "hideSettings": true
  },
  "apc.header": {
    "height": 28,
    "fontsize": 16
  },
  "apc.listRow": {
    "height": 19,
    "fontsize": 11
  },
